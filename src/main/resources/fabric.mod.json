  "entrypoints": {
    "client": [
      "me.shedaniel.clothconfig.ClothConfigInitializer"
    ]
  },
  "mixins": [
    "clothconfig.mixins.json"
  ],
  "requires": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "custom": {
    "modmenu:api": true,
    "modmenu:clientsideOnly": true
